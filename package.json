{
  "name": "redocx",
  "description": "Create word documents with React",
  "version": "1.1.4",
  "repository": "https://github.com/nitin42/redocx",
  "main": "./lib/index.js",
  "author": "Jack Tilley",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel src -d lib",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "babel-node ./demo/word.js",
    "example-mem": "babel-node ./demo/word-memory.js",
    "lint": "eslint ./src",
    "prebuild": "rm -rf ./lib",
    "dev": "npm run example | npm run start",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "browserify-fs": "^1.0.0",
    "chroma-js": "^1.3.4",
    "docx": "^4.7.0",
    "execa": "^0.8.0",
    "execa-webpack-plugin": "^2.1.2",
    "fbjs": "^0.8.4",
    "file-saver": "^2.0.0",
    "immutability-helper": "^3.0.0",
    "memory-streams": "0.1.3",
    "minimatch": "^3.0.4",
    "node-sass": "^4.11.0",
    "officegen": "^0.4.7",
    "prop-types": "^15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-reconciler": "0.2.0",
    "react-redux": "5.0.7",
    "redux": "^4.0.0",
    "semantic-ui-css": "2.3.1",
    "semantic-ui-react": "^0.84.0",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "7",
    "babel-loader": "7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.28.11",
    "diff-react": "^1.0.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "react-hot-loader": "4.2.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "^3.1.14"
  },
  "keywords": [
    "renderer",
    "react",
    "word documents",
    "custom renderer",
    "fiber"
  ]
}
